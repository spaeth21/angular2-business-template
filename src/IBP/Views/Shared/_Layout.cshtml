<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <title>@ViewData["Title"] - IBP</title>

        <environment names="Development">
            <script src="~/lib/npmlibs/es6-shim/es6-shim.js"></script>
            <script src="~/lib/npmlibs/systemjs/system-polyfills.src.js"></script>
            <script src="~/lib/npmlibs/angular2/angular2-polyfills.js"></script>

            <script src="~/lib/npmlibs/systemjs/system.src.js"></script>
            <script src="~/lib/jquery/dist/jquery.js"></script>
            <script src="~/lib/npmlibs/primeui/primeui-ng-all.js"></script>
            <script src="~/lib/npmlibs/materialize/js/materialize.js"></script>
            <script src="~/lib/npmlibs/rxjs/Rx.js"></script>
            <script src="~/lib/npmlibs/angular2/angular2.js"></script>
            <script src="~/lib/npmlibs/angular2materialize/js/materialize.js"></script>
            <script src="~/lib/npmlibs/primeui/primeui-ng.js"></script>
            <script src="~/lib/npmlibs/moment/min/moment.min.js"></script>
            <script src="~/lib/npmlibs/fullcalendar/dist/fullcalendar.js"></script>
            <script src="~/lib/npmlibs/angular2/router.js"></script>
            <script src="~/lib/npmlibs/angular2/http.js"></script>


            <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
            <link href="~/lib/npmlibs/materialize/css/materialize.css" rel="stylesheet" />
            <link href="http://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
            <link href="~/lib/npmlibs/font-awesome/css/font-awesome.css" rel="stylesheet" />
            <link href="~/lib/npmlibs/fullcalendar/dist/fullcalendar.css" rel="stylesheet" />
            <link href="~/lib/npmlibs/primeui/primeui-ng-all.css" rel="stylesheet" />
            <link href="~/css/site.css" rel="stylesheet" />

        </environment>
        <environment names="Staging,Production">
            <script src="~/lib/jquery/dist/jquery.min.js"></script>
            <script src="~/lib/npmlibs/es6-shim/es6-shim.min.js"></script>
            <script src="~/lib/npmlibs/systemjs/system-polyfills.js"></script>
